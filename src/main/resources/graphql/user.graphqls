type Query{
    findAllUsers:[User]
}
type User{
    userId:Int!,
    nickname:String!,
    phone:String!,
    password:String!,
    articles:[Article]
}
type Mutation {
    addNewUser(nickname: String!, phone: String, password: String):User!
    updateUser(userId: Int!,nickname:String,phone:String,password:String):User
}